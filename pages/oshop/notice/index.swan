<view class="page-loading" s-if="{{!hideLoading}}"></view>
<view class="prompt-panel" s-if="{{empty_list == 'YES'}}">
    <view class="icon-empty-list"></view>
</view>
<scroll-view scroll-y="true" style="height:{{sv_height}}px;" bindscrolltolower="evt_scrolltolower" s-if="{{notice_list}}">
    <view class="notice-list weui-cells">
            <navigator class="weui-cell weui-cell_access"  s-for="{{notice_list}}" s-for-item="notice" s-key="notice_id" url="detail/detail?notice_id={{notice.notice_id}}" >
                <view class="weui-cell__bd weui-cell_primary">
                    <view class="pubtime">{{notice.pubtime}}</view>
                    {{notice.title}}
                </view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
    </view>
    <view class="weui-loadmore" s-if="{{pager.current && pager.total>pager.current}}">正在加载</view>
    <view class="weui-loadmore weui-loadmore_line" s-if="{{pager.total>1 && pager.total == pager.current}}">
        <view class="weui-loadmore__tips">共{{order_count}}条</view>
    </view>
</scroll-view>
